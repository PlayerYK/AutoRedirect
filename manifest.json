{
    "version": "0.0.1",
    "manifest_version": 2,
    "name": "AutoSrc",
    "description": "auto Redirect.",
    "permissions": [
        "tabs"
        ,"webRequest"
        ,"webRequestBlocking"
        ,"http://*/*"
        ,"https://*/*"
    ],
    "options_page": "options.html",
    "background": {
        "scripts": ["jquery.min.js","background.js"]
    },
    "browser_action": {
        "default_title": "auto Redirect",
        "default_icon": {                    
            "19": "images/icon_19.png",
            "38": "images/icon_38.png"
        },
        "default_popup": "popup.html"
    }
}